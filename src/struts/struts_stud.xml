<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.devMode" value="true"></constant>
	
	<package name="stud" extends="struts-default">
		<action name="stud_*" class="crm.stud.action.studAction" method="{1}">
			<result name="findall">/pages/student/listStudent.jsp</result>
			<result name="preadd">/pages/student/addOrEditStudent.jsp</result>
		</action>
	</package>

</struts>
